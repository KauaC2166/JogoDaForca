            }

            // criacao da "palavra" que é mostrada
            resposta = new String[word[seletor].length()];

            // print das casas
            for (int pl = 0; pl < word[seletor].length(); pl++) {

                if (word[seletor].charAt(pl) == ' ') {

                    resposta[pl] = "  ";

                } else if (word[seletor].charAt(pl) == '-') {

                    resposta[pl] = "- ";

                } else if (word[seletor].charAt(pl) == '.') {

                    resposta[pl] = ". ";

                } else if (word[seletor].charAt(pl) == '/') {

                    resposta[pl] = "/ ";

                } else if (word[seletor].charAt(pl) == '!') {

                    resposta[pl] = "! ";

                } else if (word[seletor].charAt(pl) == '?') {

                    resposta[pl] = "? ";

                } else if (word[seletor].charAt(pl) == '(') {

                    resposta[pl] = "( ";

                } else if (word[seletor].charAt(pl) == ')') {

                    resposta[pl] = ") ";

                } else if (word[seletor].charAt(pl) == ',') {

                    resposta[pl] = ", ";

                } else if (word[seletor].charAt(pl) == '&') {

                    resposta[pl] = "& ";

                } else if (word[seletor].charAt(pl) == '\'') {

                    resposta[pl] = "' ";

                } else {

                    resposta[pl] = "_ ";
                }

            }

            buneco = new String[7];
            // partes do buneco inforcado
            buneco[0] = "_______\n|             |\n|            O\n|            /|\\\n|            / \\\n|";
            buneco[1] = "_______\n|             |\n|            O\n|            /|\\\n|            /\n|";
            buneco[2] = "_______\n|             |\n|            O\n|            /|\\\n|\n|";
            buneco[3] = "_______\n|             |\n|            O\n|            /|\n|\n|";
            buneco[4] = "_______\n|             |\n|            O\n|             |\n|\n|";
            buneco[5] = "_______\n|             |\n|            O\n|            \n|\n|";
            buneco[6] = "_______\n|             |\n|\n|\n|\n|";

            // parte principal
            while (tent > 0 && !venceu) {

                qntTotalChute = 0;

                letraSimples = 0;

                chuteInteiro = false;

                mensagem = " ";

                for (int y = 0; y < resposta.length; y++) {

                    mensagem = mensagem + resposta[y] + " ";
                }

                if (tent > 3) {

                    display = "Tema: " + tema + "\n" + buneco[tent] + "\n" + mensagem + "\nLetras Usadas:" + "\n" + usadas + "\n\n Digite apenas uma letra simples (sem maiusculas ou acentos) ou um numero, a não ser que queira chutar a palavra\nSe quiser chutar a palavra, digite  ela inteira, caso esteja incorreta, você perdera a partida\n Aviso: não é necessário o uso de maiusculas e assentos, incluindo “ç“";
                } else {

                    display = "Tema: " + tema + "\n" + "\nDica: " + dica + "\n" + buneco[tent] + "\n" + mensagem + "\nLetras Usadas:" + "\n" + usadas + "\n\n Digite uma letra simples (sem maiusculas ou acentos) ou um numero, a não ser que queira chutar a palavra\nSe quiser chutar a palavra, digite  ela inteira, caso esteja incorreta, você perdera a partida\n Aviso: não é necessário o uso de maiusculas e assentos, incluindo “ç“";
                }

                do {
                    // usuario digita a letra
                    chute = JOptionPane.showInputDialog(display);

                    if (chute.isEmpty()) {

                        JOptionPane.showMessageDialog(null, "Nenhuma letra foi digitada, tente novamente");

                        fzrComp = false;

                    } else {

                        if (chute.length() >= 2) {

                            confirmInteiro = JOptionPane.showConfirmDialog(null, "Você quer chutar a palavra?");

                            if (confirmInteiro == JOptionPane.YES_OPTION) {

                                chuteInteiro = true;

                                fzrComp = true;

                            } else {

                                JOptionPane.showMessageDialog(null, "Tente novamente");

                                chuteInteiro = false;

                                fzrComp = false;

                            }

                        } else {

                            fzrComp = true;

                        }
                    }

                } while (!fzrComp);

                acertou = false;

                for (int tp = 0; tp < word[seletor].length(); tp++) {

                    if (chuteInteiro) {

                        jk = tp;

                    } else {

                        jk = 0;

                    }

                    if (!chuteInteiro && chute.charAt(0) == resposta[tp].charAt(0) && !resposta[tp].equals("_ ")) {

                        JOptionPane.showMessageDialog(null, "Você já usou esta letra, tente novamente");

                        acertou = true;

                        break;

                        // comparacao
                        //a
                    } else if (chute.charAt(jk) == 'a') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == 'a') {

                            resposta[tp] = "a ";

                            acertou = true;

                        } else if (word[seletor].charAt(tp) == 'â') {

                            resposta[tp] = "â ";

                            acertou = true;

