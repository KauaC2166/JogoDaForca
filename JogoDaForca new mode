            // LIFAD Special Edition
            word[1905] = "Führe mich";
            word[1906] = "Donaukinder";
            word[1907] = "Halt";
            word[1908] = "Liese";

            //palavra personalizada
            word[1909] = " ";

            //menu de palavra selecionada
            if (escModo.equals("2")) {

                while (plvPers) {

                    wordPersonalizada = JOptionPane.showInputDialog("Digite sua palavra personalizada");

                    if (wordPersonalizada.isEmpty()) {

                        JOptionPane.showMessageDialog(null, "Nenhuma palavra foi digitada, tente novamente");

                        plvPers = true;

                    } else {

                        plvPers = false;

                        word[1909] = wordPersonalizada;

                    }
                }

                while (temaPers) {

                    temaPersonalizado = JOptionPane.showInputDialog("Digite o tema da sua palavra");

                    if (temaPersonalizado.isEmpty()) {

                        JOptionPane.showMessageDialog(null, "Nenhuma tema foi digitada, tente novamente");

                        temaPers = true;

                    } else {

                        tema = temaPersonalizado;

                        temaPers = false;
                    }
                }

                while (dicaPers) {

                    dicaPersonalizada = JOptionPane.showInputDialog("Digite uma dica para sua palavra\n(sera mostrada apos o jogador errar 3 vezes)");

                    if (dicaPersonalizada.isEmpty()) {

                        JOptionPane.showMessageDialog(null, "Nenhuma dica foi digitada, tente novamente");

                        dicaPers = true;

                    } else {

                        dica = dicaPersonalizada;

                        dicaPers = false;
                    }
                }

                seletor = 1909;

            } else if (escModo.equals("1")) {

                // sorteio da palavra
                seletor = ran.nextInt((max - min) + 1) + min;

            }

            // definicao da mensagem de tema
            if (seletor >= 0 && seletor <= 99) {

                tema = "Frutas, vegetais, grãos, ervas, etc.";

                if (seletor >= 0 && seletor <= 45) {

                    dica = "É uma fruta";

                } else if (seletor >= 46 && seletor <= 66) {

                    dica = "É um vegetal";

                } else if (seletor >= 67 && seletor <= 83) {

                    dica = "É uma erva";

                } else if (seletor >= 84 && seletor <= 92) {

                    dica = "É uma noz, cemente ou especiaria";

                } else if (seletor >= 93 && seletor <= 98) {

                    dica = "É um grão ou cereal";

                } else if (seletor == 99) {

                    dica = "É um Fungo";

                }

            } else if (seletor >= 100 && seletor <= 199) {

                tema = "Animais";

                if (seletor >= 100 && seletor <= 113) {

                    dica = "É uma ave";

                } else if (seletor >= 114 && seletor <= 115) {

                    dica = "É um camelídeo";

                } else if (seletor >= 116 && seletor <= 118) {

