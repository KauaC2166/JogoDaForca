                    } // z
                    else if (chute.charAt(jk) == 'z') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == 'z') {

                            resposta[tp] = "z ";

                            acertou = true;

                        } else if (word[seletor].charAt(tp) == 'Z') {

                            resposta[tp] = "Z ";

                            acertou = true;

                        }
                    } else if (chute.charAt(jk) == '0') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '0') {

                            resposta[tp] = "0 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '1') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '1') {

                            resposta[tp] = "1 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '2') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '2') {

                            resposta[tp] = "2 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '3') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '3') {

                            resposta[tp] = "3 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '4') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '4') {

                            resposta[tp] = "4 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '5') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '5') {

                            resposta[tp] = "5 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '6') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '6') {

                            resposta[tp] = "6 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '7') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '7') {

                            resposta[tp] = "7 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '8') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '8') {

                            resposta[tp] = "8 ";

                            acertou = true;

                        }

                    } else if (chute.charAt(jk) == '9') {

                        letraSimples++;

                        if (word[seletor].charAt(tp) == '9') {

                            resposta[tp] = "9 ";

                            acertou = true;

                        }

                    } else if (chuteInteiro) {

                        if (chute.charAt(jk) == '.' && word[seletor].charAt(tp) == '.') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '-' && word[seletor].charAt(tp) == '-') {

                            acertou = true;

                        } else if (chute.charAt(jk) == ' ' && word[seletor].charAt(tp) == ' ') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '/' && word[seletor].charAt(tp) == '/') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '!' && word[seletor].charAt(tp) == '!') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '?' && word[seletor].charAt(tp) == '?') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '(' && word[seletor].charAt(tp) == '(') {

                            acertou = true;

                        } else if (chute.charAt(jk) == ')' && word[seletor].charAt(tp) == ')') {

                            acertou = true;

                        } else if (chute.charAt(jk) == ',' && word[seletor].charAt(tp) == ',') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '&' && word[seletor].charAt(tp) == '&') {

                            acertou = true;

                        } else if (chute.charAt(jk) == '\'' && word[seletor].charAt(tp) == '\'') {

                            acertou = true;

                        }

                    } else {

                        JOptionPane.showMessageDialog(null, "Utilize apenas letras simples ou numeros, letras em maiusculas ou com acentos não são validas, incluindo 'ç'\nTente Novamente");

                        break;
                    }

                    if (chuteInteiro && acertou) {

                        qntTotalChute++;

                    }

                }

                if (chuteInteiro && qntTotalChute == resposta.length) {

                    venceu = true;

                } else if (chuteInteiro) {

                    acertou = false;
                }
                for (int du = 0; du < usadas.length(); du++) {
                    if (chute.charAt(0) == usadas.charAt(du)) {

                        JOptionPane.showMessageDialog(null, "Você já usou esta letra, tente novamente");

                        acertou = true;

                        break;
                    }
                }
                // caso tenha errado
                if (!acertou && chuteInteiro) {

                    tent = 0;

                } else if (!acertou && letraSimples == resposta.length) {

                    tent--;

                    // guarda as letras erradas ja utilizadas
                    usadas = usadas + chute.charAt(0) + " - ";
                }

                // verifica se a palavra foi completada
                venceu = true;
                for (int v = 0; v < word[seletor].length(); v++) {
                    if (resposta[v].equals("_ ")) {
                        venceu = false;
                        break;
                    }
                }

            }

            // da a mensagem final
            if (venceu) {
                // mensagem caso tenha acertado uma palavra secreta
                if (seletor >= 635 && seletor <= 639) {
                    JOptionPane.showMessageDialog(null,
                            buneco[tent] + "\nParabéns! Você encontrou uma palavra secreta! (0" + (seletor - 634) + " de 05)\nA palavra era: " + word[seletor]);
                } else {
                    // mensagem caso tenha acertado uma palavra normal
                    JOptionPane.showMessageDialog(null, buneco[tent] + "\nParabéns! Você venceu o jogo!\n A palavra era: " + word[seletor]);
                }
            } else {
                // mensagem caso tenha perdido
                JOptionPane.showMessageDialog(null, buneco[tent] + "\nVocê perdeu!\nA palavra correta era: " + word[seletor]);
            }

            reiniciar = JOptionPane.showConfirmDialog(null, "Você quer tentar novamente?");

            if (reiniciar == JOptionPane.YES_OPTION) {

                denovo = true;

            } else if (reiniciar == JOptionPane.NO_OPTION) {

                JOptionPane.showMessageDialog(null, "Obrigado por jogar!\n\nFeito por Kauã C. Seabra\n11/04/2023 - 13/04/2023\nUltima atualização: 09/06/2023");

                denovo = false;

            } else {

                denovo = false;

            }

        }
        System.out.println("\nFeito por Kauã C. Seabra\n11/04/2023 - 13/04/2023\nUltima atualização: 09/06/2023");
    }
}
